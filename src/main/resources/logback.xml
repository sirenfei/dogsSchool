<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <!-- console -->
    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <charset>UTF-8</charset>
            <pattern>%-5level%date [%thread] %F{16} [%M:%L] - %msg%n</pattern>
        </encoder>
    </appender>


    <root level="INFO">
        <appender-ref ref="console"/>
    </root>

</configuration>

